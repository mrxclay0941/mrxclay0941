import pygame

pygame.init()

screen = pygame.display.set_mode((500,600))

AQUA = (0,25,52)
WHITE = (255, 255, 255)

running = True

while running:
    screen.fill(AQUA)

    pygame.draw.rect(screen, WHITE, (100, 50, 50, 50))
    
    for event in pygame.event.get():
        if event.type == pygame.QUIT:
            running = False
        if event.type == pygame.MOUSEBUTTONDOWN:
             if event.button == 1:
                print("xxx")


        pygame.display.flip()

pygame.quit()
